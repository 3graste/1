first = ['Strings', 'Student', 'Computers']
second = ['Строка', 'Урбан', 'Компьютер']

first_result = [len(a) - len(b) for a, b in zip(first, second) if len(a) > len(b)]

first_lengths = [len(item) for item in first]

second_lengths = [len(item) for item in second]

second_result = [first_lengths[i] == second_lengths[i] for i in range(len(first_lengths))]

print(first_result)
print(second_result)
